<?xml version="1.0" encoding="utf-8"?>
<CodeTemplates version="3.0">
  <CodeTemplate version="2.0">
    <Header>
      <_Group>RFL-C#</_Group>
      <Version />
      <MimeType>text/x-csharp</MimeType>
      <Shortcut>caseState</Shortcut>
      <_Description>Add new case to FSM</_Description>
      <TemplateType>Expansion</TemplateType>
    </Header>
    <Variables>
      <Variable name="state" isIdentifier="true">
        <Default>State</Default>
      </Variable>
    </Variables>
    <Code><![CDATA[case State.$state$:
 	$selected$$end$
	break;]]></Code>
  </CodeTemplate>
